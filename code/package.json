{
  "name": "workspace",
  "version": "1.0.0",
  "description": "PNPM workspace root",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter \"apps/*\" dev",
    "build": "pnpm --filter \"apps/*\" build",
    "test": "pnpm --filter \"*\" test",
    "lint": "pnpm --filter \"*\" lint",
    "clean": "pnpm --filter \"*\" clean && rm -rf node_modules"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@10.19.0"
}